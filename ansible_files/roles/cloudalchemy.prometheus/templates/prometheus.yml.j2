global:
  scrape_interval:     15s # By default, scrape targets every 15 seconds.
scrape_configs:
  - job_name: 'prometheus'
    scrape_interval: 5s
    static_configs:
      - targets: ['34.207.245.152:9090']
  - job_name: 'node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['34.207.245.152:9100']
    metrics_path: /metrics
  - job_name: 'golang-app'
    scrape_interval: 5s
    static_configs:
      - targets: ['34.207.245.152:8080']
    metrics_path: /metrics

